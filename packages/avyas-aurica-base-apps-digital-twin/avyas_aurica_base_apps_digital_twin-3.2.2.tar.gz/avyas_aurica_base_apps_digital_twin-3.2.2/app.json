{
  "name": "digital-twin",
  "version": "3.2.2",
  "description": "Digital Life Manager - True OpenAI streaming to prevent DigitalOcean App Platform timeouts",
  "author": "Aurica Team",
  "routes": [
    {
      "path": "/api/chat",
      "file": "be/api/chat.py",
      "method": "ALL"
    },
    {
      "path": "/api/health",
      "file": "be/api/health.py",
      "method": "GET"
    }
  ],
  "public_routes": [
    "/api/health/*",
    "/api/chat/*"
  ],
  "requires": [
    "authentication"
  ],
  "dt_config": {
    "type": "digital_life_manager",
    "execution_node": true
  }
}
