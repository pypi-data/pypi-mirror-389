#A Quickstart
from epe_maria.metrics import phi, delta_phi, phi_star

f = lambda x: x**2 + 2*x + 1
g = lambda x: x**2 + x + 1

print(phi(f, g))        # Structural divergence
print(delta_phi(f, g))  # Rate divergence
print(phi_star(f, g))   # Fusion metric


