# Qdrant Configuration
# Copy this file to .env and fill in your values

# Qdrant Server URL
# For local Qdrant: http://localhost:6333
# For Qdrant Cloud: https://your-cluster.qdrant.io
QDRANT_URL=http://localhost:6333

# Qdrant API Key (optional for local, required for Qdrant Cloud)
# Leave empty for local Qdrant without authentication
QDRANT_API_KEY=

# Default collection settings
DEFAULT_DISTANCE_METRIC=COSINE  # Options: COSINE, EUCLIDEAN, DOT
