{
  "input_file": "examples/followers_following.json",
  "output_file_prefix": "Output/FollowWeb",
  "pipeline": {
    "strategy": "k-core",
    "skip_analysis": false,
    "ego_username": null
  },
  "analysis_mode": {
    "mode": "fast",
    "sampling_threshold": 3000,
    "max_layout_iterations": 50,
    "enable_fast_algorithms": true
  },
  "fame_analysis": {
    "find_paths_to_all_famous": false,
    "contact_path_target": null,
    "min_followers_in_network": 3,
    "min_fame_ratio": 3.0
  },
  "k_values": {
    "strategy_k_values": {
      "k-core": 1,
      "reciprocal_k-core": 1,
      "ego_alter_k-core": 1
    },
    "default_k_value": 1,
    "allow_cli_override": true
  },
  "visualization": {
    "node_size_metric": "degree",
    "base_node_size": 6.0,
    "node_size_multiplier": 4.0,
    "scaling_algorithm": "logarithmic",
    "base_edge_width": 0.5,
    "edge_width_multiplier": 1.5,
    "edge_width_scaling": "logarithmic",
    "intra_community_color": "#c0c0c0",
    "bridge_color": "#6e6e6e",
    "pyvis_interactive": {
      "width": "100%",
      "height": "90vh",
      "notebook": false,
      "show_labels": true,
      "show_tooltips": true,
      "physics_solver": "forceAtlas2Based"
    },
    "static_image": {
      "generate": true,
      "layout": "spring",
      "width": 1800,
      "height": 1800,
      "dpi": 150,
      "with_labels": false,
      "font_size": 8,
      "edge_alpha": 0.3,
      "node_alpha": 0.8,
      "edge_arrow_size": 6,
      "show_legend": true
    },
    "png_layout": {
      "force_spring_layout": true,
      "align_with_html": true,
      "spring": {
        "k": 0.2,
        "iterations": 30,
        "spring_length": 1.0,
        "spring_constant": 1.0,
        "repulsion_strength": 1.0,
        "attraction_strength": 1.0,
        "center_gravity": 0.01,
        "damping": 0.9,
        "min_velocity": 0.02,
        "max_displacement": 8.0,
        "enable_multistage": false
      }
    },
    "performance": {
      "fast_mode": true,
      "skip_path_analysis": false,
      "max_layout_iterations": 30,
      "use_sampling_threshold": 3000
    }
  },
  "output": {
    "custom_output_directory": null,
    "enable_time_logging": true
  }
}