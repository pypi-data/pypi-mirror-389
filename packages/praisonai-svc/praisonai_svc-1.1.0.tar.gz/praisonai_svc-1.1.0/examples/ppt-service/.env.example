# Azure Storage Configuration
PRAISONAI_AZURE_STORAGE_CONNECTION_STRING=DefaultEndpointsProtocol=https;AccountName=your_account;AccountKey=your_key;EndpointSuffix=core.windows.net

# Optional: Separate connection strings (defaults to storage connection string)
# PRAISONAI_AZURE_TABLE_CONN_STRING=
# PRAISONAI_AZURE_QUEUE_CONN_STRING=

# API Configuration
PRAISONAI_API_KEY=your_secret_api_key_here
PRAISONAI_CORS_ORIGINS=["https://your-wordpress-site.com"]

# Storage Names (optional, defaults shown)
# PRAISONAI_BLOB_CONTAINER_NAME=praison-output
# PRAISONAI_QUEUE_NAME=praison-jobs
# PRAISONAI_POISON_QUEUE_NAME=praison-jobs-poison
# PRAISONAI_TABLE_NAME=jobs

# Job Settings (optional, defaults shown)
# PRAISONAI_MAX_JOB_DURATION_MINUTES=10
# PRAISONAI_MAX_RETRY_COUNT=3
# PRAISONAI_QUEUE_VISIBILITY_TIMEOUT=60

# Worker Settings (optional, defaults shown)
# PRAISONAI_WORKER_POLL_INTERVAL_MIN=1
# PRAISONAI_WORKER_POLL_INTERVAL_MAX=30

# API Settings (optional, defaults shown)
# PRAISONAI_MAX_PAYLOAD_SIZE_MB=1
